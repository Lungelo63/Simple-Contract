pragma solidity >=0.7.0 <0.9.0;

contract HeloWorld {
    bool canEdit = true;
    bool admin = false;

    /*conditional operators
    || = or
    ? = condiotnal operator
    : = otherwise
    */
    function canEditDoc() public view returns (uint){
        uint8 result = canEdit || admin ? (true ? 1 : 0) : 0;
        return result;
        
    }
     
}
